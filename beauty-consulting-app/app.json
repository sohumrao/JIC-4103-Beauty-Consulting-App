{
	"expo": {
		"name": "beauty-consulting-app",
		"slug": "beauty-consulting-app",
		"version": "1.0.0",
		"assetBundlePatterns": ["**/*"],
		"packagerOpts": {
			"config": "metro.config.js"
		},
		"newArchEnabled": true,
		"splash": {
			"image": "./app/assets/images/splash.png"
		},
		"icon": "./app/assets/images/splash.png",
		"plugins": [
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#232323",
					"image": "./app/assets/images/splash.png",
					"imageWidth": 200
				}
			]
		]
	}
}
